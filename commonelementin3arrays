#include <bits/stdc++.h> 
vector<int> findCommonElements(vector<int> &a, vector<int> &b, vector<int> &c)
{
    int i = 0, j = 0, k = 0;
    vector<int> ans;
    while(i < a.size() && j < b.size() && k < c.size()){
        if(a[i] == b[j] && b[j] == c[k]){
            if(ans.empty() || ans.back()!= a[i]){
                ans.push_back(a[i]);
            }
            i++;
            j++;
            k++;
        }
        else if(a[i]<b[j]){
            i++;
        }
        else if(b[j]<c[k]){
            j++;
        }
        else{
            k++;
        }
    }
    return ans;
}